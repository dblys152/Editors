<script>
window.addEventListener('DOMContentLoaded', () => {
    switch('<%= flag %>') {
        case 'login': 
            alert('사용자 정보가 일치하지 않습니다.');
            history.back();
            break;
        case 'access':
            alert('해당 페이지에 접근 할 수 없습니다.');
            history.back();
            break;
        case 'pstNo':
            alert('게시글 번호가 잘못되었거나 게시글이 존재하지 않습니다.');
            history.back();
            break;
        default:
            alert('해당 플래그를 찾을 수 없습니다.');
            history.back();
    }
});
</script>